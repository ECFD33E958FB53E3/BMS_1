local playerArm = game.Players.LocalPlayer.Character['Torso']
local UIS = game:GetService("UserInputService")

UIS.InputBegan:connect(function(lol)
if lol.KeyCode == Enum.KeyCode.R then
if _G.settingstable.ballmag == true then
for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
    if v.Name == "TouchInterest" and v.Parent.Name == "Handle" then
        firetouchinterest(playerArm, v.Parent, 0)
        wait(0.1)
        firetouchinterest(playerArm, v.Parent, 1)
else
return nil
    end
end
end
end
end)
